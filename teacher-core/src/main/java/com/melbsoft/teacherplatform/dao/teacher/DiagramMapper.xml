<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.melbsoft.teacherplatform.dao.teacher.DiagramMapper">

    <select id="query" resultType="DiagramObject">
        SELECT object_content as content
        from diagram_object
        where object_id=#{q.index}
    </select>

</mapper>
